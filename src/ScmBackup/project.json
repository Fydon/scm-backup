    "emitEntryPoint": true,
    "copyToOutput": { "includeFiles": [ "settings.json", "NLog.config" ] }
  },
  "publishOptions": {
    "includeFiles": [ "ScmBackup.bat", "settings.json", "NLog.config" ]
  },

  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Binder": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "Newtonsoft.Json": "9.0.1",
    "NLog": "4.4.0-betaV15",
    "SimpleInjector": "3.2.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  }
}
